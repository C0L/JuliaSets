#!/bin/bash
#SBATCH --job-name="Julia Set"
#SBATCH --output="juliaset.%j.%N.out"
#SBATCH --partition=compute
#SBATCH --nodes=1
#SBATCH --ntasks 16
#SBATCH --export=ALL
#SBATCH -t 00:04:00
#SBATCH -A csd453

module purge 
module load slurm 
module load cpu
module load aocc
module load openmpi/4.0.4

mpirun -n 16 ./juliaset -i 1000 -n 16 -s 4096 -r 1.3 -e 3 -o "single/jset_output"
