#!/bin/bash
#SBATCH --job-name="Julia Set"
#SBATCH --output="juliaset_tau.out"
#SBATCH --partition=compute
#SBATCH --nodes=8
#SBATCH --ntasks 1024
#SBATCH --export=ALL
#SBATCH -t 00:25:00
#SBATCH -A csd453

module purge 
module load slurm 
module load cpu
module load aocc
module load openmpi/4.0.4

mpirun -n 1024 ./juliaset -i 10000 -n 1024 -s 16384 -r 1.3 -e 3
