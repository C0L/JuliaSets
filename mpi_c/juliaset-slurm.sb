#!/bin/bash
#SBATCH --job-name="Julia Set"
#SBATCH --output="juliaset.%j.%N.out"
#SBATCH --partition=compute
#SBATCH --nodes=1
#SBATCH --ntasks 4
####SBATCH --nodes=3
####SBATCH --ntasks 12
#SBATCH --export=ALL
#SBATCH -t 00:04:00
#SBATCH -A csd453

 module purge 
 module load slurm 
 module load cpu
 module load aocc
 module load openmpi/4.0.4

srun --mpi=pmi2 -n 4 --cpu-bind=rank ./juliaset -i 10000 -n 1 -s 100 -r 1.3 -e 3
